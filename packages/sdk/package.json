{
  "name": "@stability/sdk",
  "packageManager": "yarn@3.3.0",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf node_modules",
    "lint": "eslint \"./src/**/*.{ts}\"",
    "lint:fix": "yarn lint --fix",
    "generate:rest-api-types": "openapi-typescript ./src/API/OpenAPI.json --output ./src/API/OpenAPI.generated.ts",
    "build:types": "yarn generate:rest-api-types && tsc --project tsconfig.json && tsc-alias",
    "build:javascript": "tsx scripts/Build.ts",
    "build": "yarn build:types && yarn build:javascript",
    "dev": "nodemon --watch src --ext ts,json --exec \"yarn build\" --ignore \"./src/**/*.generated.*\""
  },
  "devDependencies": {
    "@stability/sdk": "workspace:^",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "nodemon": "^2.0.22",
    "openapi-typescript": "latest",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.6",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  }
}
